{
  "editor.tabSize": 2,
  "editor.fontWeight": "500",
  "terminal.integrated.fontWeightBold": "900",
  "terminal.integrated.fontSize": 16,
  "editor.fontSize": 16,
  "workbench.iconTheme": "vscode-simpler-icons",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.fontFamily": "Consolas, monospace, 'Courier New'",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "bracketPairColorizer.consecutivePairColors": [
    [":", ";"],
    ["<", "</"],
    ["<", "/>"],
    ["<", ">"],
    ["(", ")"],
    ["{", "}"],
    ["[", "]"],
    ["lightgreen"],
    "lightgreen"
  ],
  "workbench.colorCustomizations": {},
  "terminal.integrated.rendererType": "dom",
  "vscode_vibrancy.opacity": -1,
  "vscode_vibrancy.theme": "Default Dark",
  "vscode_vibrancy.type": "medium-light",
  "explorer.confirmDelete": false,
  "tslint.jsEnable": true,
  "tslint.suppressWhileTypeErrorsPresent": true
}
